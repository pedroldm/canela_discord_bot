        "pichau.com.br" : ["vista.+?(?=\\d+\\.\\d+)(\\d+\\.\\d+)"],
        "terabyteshop.com.br" : ["valVista.+?(?=\\d+\\.\\d+)(\\d+\\.\\d+)"],
        "store.steampowered.com" : ["data-price-final[^>]*>\\s*R\\$\\s*([\\d\\.\\,]*)", "discount_final_price\">R\\$\\s*([\\d\\,\\.]*)"]
    }
}